color-primary = #f1c40f
color-heading = #666

body
  background-color: #999

ul
  list-style: none

a
  text-decoration: none
  color: color-heading

.color--primary
  color: color-primary

.row
  width: 100%
  max-width: 960px
  margin: 0 auto
  box-sizing: border-box

[class*="col-"]
  box-sizing: border-box
  display: inline-block

.col-1-2
  width: 50%